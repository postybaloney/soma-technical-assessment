generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Todo {
  id        Int      @id @default(autoincrement())
  title     String
  createdAt DateTime  @default(now())
  dueDate   DateTime? // Optional due date for the task
  imageUrl  String?   // Optional image URL for the task
}